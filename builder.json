[
    {
        "name": "PHP",
        "cmd": "php $FILE",
        "file_reg": "in( file)? ([^\\s]+) ",
        "line_reg": "on line ([0-9]+)",
        "err_reg": "(.*) in"
    },
    {
        "name": "JavaScript",
        "cmd": "node $FILE",
        "filereg": " ([^\\s]+):",
        "linereg": "js:([0-9]+)"
    },
    {
        "name": "_Ruby",
        "cmd": "ruby $FILE",
        "filereg": "todo",
        "linereg": ":([0-9]+):"
    },
    {
        "name": "_Python",
        "cmd": "python $FILE",
        "filereg": "todo",
        "linereg": "line ([0-9]+)"
    }
]